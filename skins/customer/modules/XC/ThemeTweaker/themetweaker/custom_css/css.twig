 # Header part
 #
 # @ListChild (list="head.css", weight="9999")
 #}

{% if this.isInCustomCssMode() %}

    {% if this.isCustomCssEnabled() %}
        <style rel="stylesheet" media="screen" data-custom-css>
        {{ this.getCustomCssText() }}
        </style>
    {% else %}
        <script data-custom-css>
        {{ this.getCustomCssText() }}
        </script>
    {% endif %}
{% endif %}