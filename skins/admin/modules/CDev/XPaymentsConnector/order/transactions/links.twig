{##
 # Links for XPC payment transaction 
 #
 #}
<div class="links-container {% if this.entity.transaction.isFraudStatus() and not this.entity.transaction.isPendingFraudCheck() %}pull-right{% endif %}">
  <a class="dotted" href="javascript: void(0);" onclick="javascript: showAddInfo({{ this.getOrderNumber() }}, {{ this.getTransactionId(this.entity) }});">{{ t('View payment information') }}</a>
  <a href="{{ this.getPaymentURL(this.entity) }}" target="blank">{{ t('Go to payment details page') }}</a>
</div>
